/*  设置轮播图的宽高，需要也给swiper和image设置height值 */
.swiperView swiper{
  height: 360rpx;
}
.swiperView image{
  width: 100%;
  height: 360rpx;
}

/* 九宫格 */
/* 
  .comMsg{
    display: flex;
    flex-wrap: wrap
  }
 */
.iconMsg {
  width: 100%;
  background-color: #fff;
}

/* 怎么控制这个？？？刚好宽度 */
.iconMsg navigator{
  /* width: 33.1%  */
  width: 251rpx;
  height: 200rpx;
  /* border: 1rpx solid #ccc; */
  border-right: 1rpx solid #ccc;
  border-bottom: 1rpx solid #ccc;
  border-top: 0;
  float: left;
  justify-content: space-around;
  line-height: 1.4;
  text-align: center;
  box-sizing: border-box;
}

/* .iconMsg navigator:nth-child(n){
  border-left: none;
} */

.iconMsg navigator:nth-child(3n){
  border-right: none;
}


.iconMsg navigator image{
  width: 100rpx;
  height: 100rpx;
  display: block;
  margin: 16rpx auto;
}

.iconMsg navigator text{
  font-size: 30rpx;
}

/* 首页两大链接样式 */
.navMsg{
  display: flex;
  margin-bottom: 20rpx;
}

.navMsg image{
  flex: 1;
  width: 310rpx;
  height: 200rpx;
  margin: 20rpx;
}